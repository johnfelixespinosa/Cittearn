<strong>Welcome Back!</strong> <%= current_user.name %>

<div>
  <h3>Your Songs</h3><br>
  <% current_user.songs.each do |song| %>
    <div>
      <h2 class=strong><%= link_to song.title, song_path(song) %></h2>
      <%= link_to "Edit Song", edit_song_path, class: "btn" %>
      <%= link_to "Delete Song", song_path(song), 
        data: {confirm: "Are you sure?"}, :method => :delete %> 
    </div>
  <% end %>